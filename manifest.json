{
  "manifest_version": 2,
  "name": "SAVER",
  "description": "Easiest way to catch up on your interests!",
  "version": "1.1",
  "permissions": [
      "bookmarks",
      "tabs"
   ],
  "browser_action": {
      "default_title": "SAVER Bookmarks",
      "default_icon": "icon.png"
    },
  "background": {
    "scripts": ["onClick.js"],
    "persistent": false
  },
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}